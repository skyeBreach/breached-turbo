{
    "ui": "tui",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "inputs": ["$TURBO_DEFAULT$"],
            "outputs": ["dist/**", "*.tsbuildinfo"]
        },
        "typecheck": {
            "dependsOn": ["^build"],
            "inputs": ["$TURBO_DEFAULT$"],
            "outputs": ["*.tsbuildinfo"]
        },
        "dev": {
            "dependsOn": ["^build"],
            "persistent": true,
            "cache": false,
            "inputs": ["$TURBO_DEFAULT$"]
        },
        "lint": {
            "dependsOn": ["@breached-turbo/config-eslint#build"],
            "inputs": ["$TURBO_DEFAULT$"]
        }
    }
}
